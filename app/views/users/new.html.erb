<h1> Sign up and be a Cookie Monster Too!! </h1>

<%= form_for(@user) do |f|%>

  <%= f.label :name %>
  <%= f.text_field :name %><br>

    <%= f.label :email %>
    <%= f.text_field :email %><br>

    <%= f.label :username %>
    <%= f.text_field :username %><br>
  
    <%= f.label :password %>
    <%= f.password_field :password %><br>
  <br>
  
    <%= f.submit "Om Nom Nom Nom" %>

<h5> Have an account already? <%= link_to "Login", login_path %> </h5>
  <% end %>
